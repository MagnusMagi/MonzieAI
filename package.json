{
  "name": "monzieai",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --lan",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace monzieai.xcworkspace -scheme monzieai -configuration Release archive",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "maestro test .maestro/",
    "analyze:bundle": "node scripts/analyze-bundle-size.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\" \"*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\" \"*.{ts,tsx,json}\""
